{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": { "browsers": "> 1%" },
      "useBuiltIns": "entry",
      "forceAllTransforms": true
    }],
    "vue"
  ],

  "plugins": [
    "syntax-dynamic-import",
    ["transform-class-properties", { "spec": true }]
  ]
}
